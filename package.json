{
  "name": "tfmonitor",
  "version": "0.0.2",
  "description": "Automatically update and restart node applications when you push to GIT.",
  "repository": "git://github.com/twofront/TF-Monitor",
  "author": "Thomas Baron",
  "license": "MIT",
  "main": "index.js",
  "engine": "node >= 0.4.1",
  "preferGlobal": "true",
  "bin": {
    "tfmonitor": "bin/tfmonitor.js"
  },
  "dependencies": {
    "stdio": "~0.1.5"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": true,
    "maxparams": 4,
    "maxdepth": 5,
    "maxlen": 130,
    "globalstrict": true,
    "asi": false,
    "eqnull": false,
    "evil": false,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": true,
    "devel": true,
    "browser": true,
    "node": true
  }
}
